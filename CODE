n = int(input('Digite um nÃºmero inteiro: '))
n = str(n)
nl = list(n)
nlen = len(nl)
n_list = [11]
while nlen > 0:
    n = int(n)
    a = n%10
    a = str(a)
    n_list.append(a)
    n = n//10
    nlen -= 1
    if n_list[-1] == n_list[-2]:
        print('sim')
        break
    elif nlen == 0:
        print('nÃ£o')
